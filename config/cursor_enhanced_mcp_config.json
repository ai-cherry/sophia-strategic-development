{
  "mcpServers": {
    "n8n_gateway": {
      "command": "python",
      "args": [
        "n8n-integration/n8n_mcp_gateway.py"
      ],
      "env": {
        "ENVIRONMENT": "prod",
        "PULUMI_ORG": "scoobyjava-org",
        "PYTHONPATH": "."
      },
      "disabled": false,
      "alwaysAllow": [
        "*"
      ],
      "initializationOptions": {
        "auto_start": true,
        "health_check_url": "http://localhost:8100/health"
      }
    },
    "snowflake_unified": {
      "command": "python",
      "args": [
        "mcp-servers/snowflake_unified/unified_snowflake_server.py"
      ],
      "env": {
        "PORT": "9030"
      }
    },
    "slack_unified": {
      "command": "python",
      "args": [
        "mcp-servers/slack_unified/slack_integration_server.py"
      ],
      "env": {
        "PORT": "9031"
      }
    },
    "sophia_intelligence_unified": {
      "command": "python",
      "args": [
        "mcp-servers/sophia_intelligence_unified/unified_sophia_intelligence_server.py"
      ],
      "env": {
        "PORT": "9032"
      }
    },
    "hubspot_unified": {
      "command": "python",
      "args": [
        "mcp-servers/hubspot_unified/hubspot_server.py"
      ],
      "env": {
        "PORT": "9033"
      }
    }
  },
  "version": "4.0-phoenix",
  "description": "Cursor IDE MCP Configuration - Unified N8N Gateway",
  "last_updated": "2024-07-02T12:00:00.000000Z"
}