# Notion_V2 MCP Server Environment Variables

# Server Configuration
NOTION_V2_PORT=9001
NOTION_V2_LOG_LEVEL=INFO

# API Configuration
NOTION_V2_API_KEY=your_api_key_here
NOTION_V2_API_BASE_URL=https://api.notion_v2.com/v1

# Database Configuration (optional)
NOTION_V2_DB_DSN=postgresql+asyncpg://user:pass@localhost:5432/notion_v2_db

# Feature Flags
NOTION_V2_ENABLE_AI_PROCESSING=true
NOTION_V2_ENABLE_METRICS=true
