# Github MCP Server Environment Variables

# Server Configuration
GITHUB_PORT=9001
GITHUB_LOG_LEVEL=INFO

# API Configuration
GITHUB_API_KEY=your_api_key_here
GITHUB_API_BASE_URL=https://api.github.com/v1

# Database Configuration (optional)
GITHUB_DB_DSN=postgresql+asyncpg://user:pass@localhost:5432/github_db

# Feature Flags
GITHUB_ENABLE_AI_PROCESSING=true
GITHUB_ENABLE_METRICS=true
