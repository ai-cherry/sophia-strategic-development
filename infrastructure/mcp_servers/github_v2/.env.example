# Github_V2 MCP Server Environment Variables

# Server Configuration
GITHUB_V2_PORT=9001
GITHUB_V2_LOG_LEVEL=INFO

# API Configuration
GITHUB_V2_API_KEY=your_api_key_here
GITHUB_V2_API_BASE_URL=https://api.github_v2.com/v1

# Database Configuration (optional)
GITHUB_V2_DB_DSN=postgresql+asyncpg://user:pass@localhost:5432/github_v2_db

# Feature Flags
GITHUB_V2_ENABLE_AI_PROCESSING=true
GITHUB_V2_ENABLE_METRICS=true
