# Slack_V2 MCP Server Environment Variables

# Server Configuration
SLACK_V2_PORT=9001
SLACK_V2_LOG_LEVEL=INFO

# API Configuration
SLACK_V2_API_KEY=your_api_key_here
SLACK_V2_API_BASE_URL=https://api.slack_v2.com/v1

# Database Configuration (optional)
SLACK_V2_DB_DSN=postgresql+asyncpg://user:pass@localhost:5432/slack_v2_db

# Feature Flags
SLACK_V2_ENABLE_AI_PROCESSING=true
SLACK_V2_ENABLE_METRICS=true
