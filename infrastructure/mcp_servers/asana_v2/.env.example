# Asana_V2 MCP Server Environment Variables

# Server Configuration
ASANA_V2_PORT=9001
ASANA_V2_LOG_LEVEL=INFO

# API Configuration
ASANA_V2_API_KEY=your_api_key_here
ASANA_V2_API_BASE_URL=https://api.asana_v2.com/v1

# Database Configuration (optional)
ASANA_V2_DB_DSN=postgresql+asyncpg://user:pass@localhost:5432/asana_v2_db

# Feature Flags
ASANA_V2_ENABLE_AI_PROCESSING=true
ASANA_V2_ENABLE_METRICS=true
