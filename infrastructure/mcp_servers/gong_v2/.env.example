# Gong_V2 MCP Server Environment Variables

# Server Configuration
GONG_V2_PORT=9001
GONG_V2_LOG_LEVEL=INFO

# API Configuration
GONG_V2_API_KEY=your_api_key_here
GONG_V2_API_BASE_URL=https://api.gong_v2.com/v1

# Database Configuration (optional)
GONG_V2_DB_DSN=postgresql+asyncpg://user:pass@localhost:5432/gong_v2_db

# Feature Flags
GONG_V2_ENABLE_AI_PROCESSING=true
GONG_V2_ENABLE_METRICS=true
