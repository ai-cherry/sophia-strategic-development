# Codacy_V2 MCP Server Environment Variables

# Server Configuration
CODACY_V2_PORT=9001
CODACY_V2_LOG_LEVEL=INFO

# API Configuration
CODACY_V2_API_KEY=your_api_key_here
CODACY_V2_API_BASE_URL=https://api.codacy_v2.com/v1

# Database Configuration (optional)
CODACY_V2_DB_DSN=postgresql+asyncpg://user:pass@localhost:5432/codacy_v2_db

# Feature Flags
CODACY_V2_ENABLE_AI_PROCESSING=true
CODACY_V2_ENABLE_METRICS=true
