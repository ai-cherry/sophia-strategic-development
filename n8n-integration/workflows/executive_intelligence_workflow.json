          "response": {
            "response": {
              "responseFormat": "json"
            }
          }
        }
      },
      "name": "Fetch LinkedIn Ads",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [300, 200],
      "id": "linkedin-fetch"
    },
    {
      "parameters": {
        "url": "https://googleads.googleapis.com/v14/customers",
        "authentication": "oAuth2",
        "options": {
          "response": {
            "response": {
              "responseFormat": "json"
            }
          }
        }
      },
      "name": "Fetch Google Ads",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [300, 400],
      "id": "google-ads-fetch"
    },
    {
      "parameters": {
        "url": "http://sophia-mcp-bridge:9099/api/v1/n8n/process",
        "options": {
          "body": {
            "body": {
              "workflow_id": "executive_intelligence",
              "execution_id": "={{$json.execution_id}}",
              "node_name": "LinkedIn_AI_Enhancement",
              "mcp_server": "business_intelligence",
              "enhancement_type": "executive_intelligence",
              "data": "={{$json}}",
              "priority": "executive"
            }
          }
        },
        "method": "POST"
      },
      "name": "Sophia AI Enhancement - LinkedIn",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [500, 200],
      "id": "sophia-linkedin-enhancement"
    },
    {
      "parameters": {
        "url": "http://sophia-mcp-bridge:9099/api/v1/n8n/process",
        "options": {
          "body": {
            "body": {
              "workflow_id": "executive_intelligence",
              "execution_id": "={{$json.execution_id}}",
              "node_name": "Google_AI_Enhancement",
              "mcp_server": "business_intelligence", 
              "enhancement_type": "executive_intelligence",
              "data": "={{$json}}",
              "priority": "executive"
            }
          }
        },
        "method": "POST"
      },
      "name": "Sophia AI Enhancement - Google",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [500, 400],
      "id": "sophia-google-enhancement"
    },
    {
      "parameters": {
        "url": "http://sophia-mcp-bridge:9099/api/v1/n8n/batch-process",
        "options": {
          "body": {
            "body": {
              "requests": [
                {
                  "workflow_id": "cross_platform_synthesis",
                  "execution_id": "={{$json.execution_id}}",
                  "node_name": "Executive_Synthesis",
                  "mcp_server": "ai_memory",
                  "enhancement_type": "cross_platform_correlation",
                  "data": {
                    "linkedin_insights": "={{$node['Sophia AI Enhancement - LinkedIn'].json}}",
                    "google_insights": "={{$node['Sophia AI Enhancement - Google'].json}}",
                    "synthesis_type": "executive_dashboard"
                  },
                  "priority": "executive"
                }
              ]
            }
          }
        },
        "method": "POST"
      },
      "name": "Cross-Platform Intelligence Synthesis",
      "type": "n8n-nodes-base.httpRequest", 
      "typeVersion": 1,
      "position": [700, 300],
      "id": "cross-platform-synthesis"
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{$json.risk_score}}",
              "operation": "larger",
              "value2": "7"
            }
          ]
        }
      },
      "name": "High Risk Alert?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 1,
      "position": [900, 300],
      "id": "risk-assessment"
    },
