{
  "name": "Executive Intelligence Pipeline",
  "description": "Real-time cross-platform business intelligence impossible with standard integrations",
  "nodes": [
    {
      "parameters": {
        "cronExpression": "0 */6 * * *",
        "triggerAtStartup": true
      },
      "name": "Every 6 Hours",
      "type": "n8n-nodes-base.cron",
      "typeVersion": 1,
      "position": [100, 300],
      "id": "cron-trigger"
    },
    {
      "parameters": {
        "url": "https://api.linkedin.com/v2/adCampaignsV2",
        "authentication": "oAuth2",
